{
  "name": "@fjell/client-api",
  "description": "Client API for Fjell",
  "version": "4.4.12",
  "keywords": [
    "client",
    "api",
    "fjell"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"tsc --noEmit --watch\" \"vite build --watch\"",
    "build": "pnpm run lint && tsc --noEmit && vite build",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "docs:dev": "cd docs && pnpm run dev",
    "docs:build": "cd docs && pnpm run build",
    "docs:preview": "cd docs && pnpm run preview",
    "docs:test": "cd docs && pnpm run test"
  },
  "dependencies": {
    "@fjell/core": "^4.4.13",
    "@fjell/http-api": "^4.4.15",
    "@fjell/logging": "^4.4.13",
    "@fjell/registry": "^4.4.11",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@fjell/eslint-config": "^1.0.5",
    "@swc/core": "^1.13.1",
    "@tsconfig/recommended": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.31.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.13.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/client-api.git"
  }
}

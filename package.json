{
  "name": "@fjell/client-api",
  "description": "Client API for Fjell",
  "version": "4.4.15",
  "keywords": [
    "client",
    "api",
    "fjell"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"tsc --noEmit --watch\" \"node build.js --watch\"",
    "build": "npm run lint && tsc --noEmit && node build.js",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run clean && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/core": "^4.4.25",
    "@fjell/http-api": "^4.4.23",
    "@fjell/logging": "^4.4.30",
    "@fjell/registry": "^4.4.20",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@fjell/eslint-config": "^1.1.3",
    "@tsconfig/recommended": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/client-api.git"
  }
}

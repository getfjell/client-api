{
  "name": "@fjell/client-api",
  "description": "Client API for Fjell",
  "version": "4.4.2",
  "license": "Apache-2.0",
  "type": "module",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "concurrently \"tsc --noEmit --watch\" \"vite build --watch\"",
    "build": "pnpm run lint && tsc --noEmit && vite build",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@fjell/core": "^4.4.3",
    "@fjell/http-api": "^4.4.1",
    "@fjell/logging": "^4.4.3",
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@swc/core": "^1.12.5",
    "@tsconfig/recommended": "^1.0.10",
    "@types/luxon": "^3.6.2",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node": "^5.0.1",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.11.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/client-api.git"
  }
}
